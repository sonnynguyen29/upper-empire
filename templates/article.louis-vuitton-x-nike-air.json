    "blog_content_YBFm3J": {
      "type": "blog--content",
      "blocks": {
        "text_VptFUj": {
          "type": "text",
          "settings": {
            "text": "<p>Every detail reflects deliberate craftsmanship: custom lace tags engraved with \"Air Louis,\" monogrammed hangtags, and opulent packaging echoing the elegance of Louis Vuitton trunks. This is more than footwear—it’s a declaration of style, culture, and history.</p>"
          }
        }
      },
